  {
    "name": "Calendar_Extension",
    "version": "1.0",
    "description": "Calendar Extension",
    "permissions": [
      "storage", "activeTab", "http://*/*", "https://*/*"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "calendar.html"
    },
    "manifest_version": 2
  }